bring ERR


fun div(a: int, b: int) -> deci,ERR::Error {
  if(b == 0) {
    return 0.0, new ERR::Error("Can not divide "+a+" with 0")
  }
  return a/b, Nir
}
var a:int,b:int 

a = Int32(input("Enter Value for a: "))
b = Int32(input("Enter value for b: "))


var ans:deci,err:ERR::Error = (div(a,b))

if(err == Nir) {
  print(ans)
}else{
    print(err.message)
  }



































































































































































































